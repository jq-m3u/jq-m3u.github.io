{
  "manifest_version": 2,
  "name": "Start Page",
  "short_name": "Start Page",
  "version": "1.4.2",
  "description": "New Tab",
  "applications": {
    "gecko": {
      "id": "jq@jq.com",
      "strict_min_version": "58.0"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },

  "icons": {
    "512": "logo512.png"
  },
  "permissions": ["*://google.com/*"],
  "omnibox": {
    "keyword": "newtab"
  },
  "web_accessible_resources": [
    "google.com"
  ],
  "content_security_policy": "script-src 'self' https://duckduckgo.com; object-src 'self' https://duckduckgo.com;",
  "permissions": ["http://*/*", "storage", "activeTab"]
}
